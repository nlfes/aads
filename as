local Section = Tab:NewSection("Auto Dropper")
Section:NewToggle("Auto Drop The Banana", "auto drops the banana", function(state)
    if state then
        print("Toggle On")
        getgenv().farme = true;

while wait(1) do
    if getgenv().farme == true then
        local args = {
            [1] = "The banana"
        }
        
        game:GetService("ReplicatedStorage").RemoteEvents.Equip:FireServer(unpack(args))
        wait(1)
         local args = {
            [1] = "The banana"
}

game:GetService("ReplicatedStorage").RemoteEvents.Drop:FireServer(unpack(args))

    end
end
    else
