local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Pop It Script v2.2", "Ocean")

local Tab = Window:NewTab("main")
local Section = Tab:NewSection("main")
Section:NewTextBox("Buy Any Item", "Type in the box what you want then hit enter", function(txt)
    print(txt)
    local args = {
        [1] = (txt)
    }
    
    game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end)
Section:NewToggle("Auto Water Melon", "auto equip water melon loop", function(state)
    if state then
        print("Toggle On")
