local Tab = Window:NewTab("Misc")
local Misc = Tab:NewSection("Misc")
Misc:NewButton("E to fly script", "Makes you fly", function()
        loadstring(game:HttpGet(('https://pastebin.com/raw/KtLKcbxr'),true))()
    print("Clicked")
end)
Misc:NewSlider("WalkSpeed", "makes speed faster monke", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
Misc:NewSlider("JumpPower", "your dumb", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
Misc:NewKeybind("Make a bind to open and close GUI", "open n close ui", Enum.KeyCode.F, function()
    Library:ToggleUI()
end)
local Tab = Window:NewTab("Info")
local Info = Tab:NewSection("Info")
Info:NewTextBox("Made by 16#0001", "TextboxInfo", function(txt)
    print(txt)
end)
